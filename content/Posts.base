views:
  - type: table
    name: 表格
    filters:
      and:
        - file.folder == "content/blogs"
    order:
      - title
      - file.name
      - file.ctime
      - tags
    sort:
      - property: file.mtime
        direction: DESC
    indentProperties: false
    separator: " "
    markers: number
    columnSize:
      file.name: 175
